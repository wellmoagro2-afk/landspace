{
  "name": "landspace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "npm run check:env && next build --webpack",
    "start": "npm run check:env && next start",
    "lint": "eslint",
    "check:env": "node scripts/check-env.mjs",
    "check:csp-console": "node scripts/csp-console-check.mjs",
    "check:csp-routes": "node scripts/csp-routes-check.mjs",
    "check:csp-routes:prod": "npm run build && start-server-and-test \"next start -p 3000\" http://localhost:3000 \"node scripts/csp-routes-check.mjs\"",
    "check:csp-prod": "npm run build && start-server-and-test \"next start -p 3001\" http://localhost:3001 \"BASE_URL=http://localhost:3001 node scripts/csp-prod-check.mjs\"",
    "qa:csp": "node scripts/qa-csp.mjs",
    "db:generate": "prisma generate --schema prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "db:push": "prisma db push --schema prisma/schema.prisma",
    "db:studio": "prisma studio --schema prisma/schema.prisma",
    "db:seed": "tsx prisma/seed.ts",
    "db:check": "node scripts/check-db.mjs",
    "dev:3001": "next dev -p 3001",
    "check:ssrf": "node scripts/ssrf-check.mjs",
    "xss:check": "node scripts/xss-sanitize-check.mjs",
    "mdx:check": "node scripts/mdx-security-check.mjs",
    "test:admin-login": "node scripts/test-admin-login.mjs"
  },
  "dependencies": {
    "@keystatic/core": "^0.5.48",
    "@keystatic/next": "^5.0.4",
    "@prisma/client": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/ioredis": "^4.28.10",
    "bcryptjs": "^3.0.3",
    "gray-matter": "^4.0.3",
    "ioredis": "^5.9.2",
    "iron-session": "^8.0.4",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "maplibre-gl": "^4.7.1",
    "next": "16.1.1",
    "next-mdx-remote": "^5.0.0",
    "prisma": "^6.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sanitize-html": "^2.17.0",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sanitize-html": "^2.16.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "playwright": "^1.57.0",
    "start-server-and-test": "^2.0.5",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
